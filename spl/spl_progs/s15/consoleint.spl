alias currentPID R0;
currentPID = [SYSTEM_STATUS_TABLE + 1];

[PROCESS_TABLE + currentPID * 16 + 13] = SP;

SP = [PROCESS_TABLE + currentPID * 16 + 11] * 512 - 1;

backup;

alias reqPID R1;
reqPID = [TERMINAL_STATUS_TABLE + 1];

[PROCESS_TABLE + reqPID * 16 + 8] = P0;

multipush(R0,R1);
R1 = 9;
R2 = reqPID;
call MOD_0;

multipop(R0,R1);

restore;

SP = [PROCESS_TABLE + currentPID * 16 + 13];

ireturn;