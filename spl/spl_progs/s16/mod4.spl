//Device Manager Module
// print "In Module 4";
alias functionNum R1;
alias currentPID R2;

if (functionNum == 3) then
    // calling acquire Terminal
    multipush(R1,R2,R3);
    R1 = 8;
    R2 = currentPID; //same thing, just for reference
    call MOD_0;
    multipop(R1,R2,R3);
    print R3;

    //releasing Terminal
    multipush(R1,R2,R3);
    R1 = 9;
    R2 = currentPID;
    call MOD_0;
    multipop(R1,R2,R3);
    return;
else
    return;
endif;
